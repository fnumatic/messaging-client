(defn path [d]
  [:path {:stroke-linecap  "round"
          :stroke-linejoin "round"
          :stroke-width    "2"
          :d             d}])

(defn svg [opts d]
  [:svg (merge {:class   "flex-none w-3 h-3"
                :fill    "none"
                :stroke  "currentColor"
                :viewBox "0 0 24 24"
                :xmlns   "http://www.w3.org/2000/svg"}
               opts)
   (if (sequential? d) 
     (spread-by-order path d)
     [path d])])
